{
  "name": "@event-mgmt/shared-schemas",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "declaration": true,
  "declarationMap": true,
  "outDir": "./dist",
  "rootDir": "./src",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "eslint ."
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.0.0"
  }
}
